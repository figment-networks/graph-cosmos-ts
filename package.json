{
  "name": "@graphprotocol/cosmoshub-ts",
  "version":"0.1.0-alpha.0",
  "license": "MIT",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "ascMain": "src/index.ts",
  "scripts": {
    "prebuild": "cd ./node_modules/as-proto-gen && yarn build --scope as-proto-gen",
    "build-cosmos": "protoc @cosmos-option.txt --as_out=./src $(./cosmos-file-list.sh)",
    "build-ibc": "protoc @ibc-option.txt --as_out=./src $(./ibc-file-list.sh)",
    "postbuild-ibc": "sed -i -e 's[../../../../proofs[../../../../confio[' src/ibc/core/commitment/v1/commitment.ts",
    "build": "yarn build-cosmos && yarn build-ibc",
    "test": "asc --explicitStart --exportRuntime --runtime stub src/index.ts --lib graph-cosmos-ts -b index.wasm"
  },
  "dependencies": {
    "as-proto": "^0.2.3"
  },
  "devDependencies": {
    "as-proto-gen": "https://github.com/figment-networks/as-proto.git#v0.3.0-alpha.0",
    "assemblyscript": "^0.19.10"
  }
}
