{
  "name": "@graphprotocol/cosmos-ts",
  "version": "0.1.0-alpha.0",
  "license": "MIT",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "ascMain": "src/index.ts",
  "scripts": {
    "prebuild": "cd ./node_modules/as-proto-gen && yarn build --scope as-proto-gen",
    "build": "protoc --plugin=protoc-gen-as=./node_modules/as-proto-gen/packages/as-proto-gen/bin/as-proto-gen --proto_path=./third_party/ --as_out=./src $(./proto-file-list.sh)",
    "test": "asc --explicitStart --exportRuntime --runtime stub src/index.ts --lib graph-cosmos-ts -b index.wasm"
  },
  "dependencies": {
    "as-proto": "^0.2.3"
  },
  "devDependencies": {
    "as-proto-gen": "https://github.com/figment-networks/as-proto.git#v0.3.0-alpha.0",
    "assemblyscript": "^0.19.10"
  }
}
